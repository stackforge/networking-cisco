[Unit]
Description=OpenStack Neutron Cisco Configuration Agent
After=syslog.target network.target

[Service]
Type=simple
User=neutron
ExecStart=/usr/bin/neutron-cisco-cfg-agent --config-file /usr/share/neutron/neutron-dist.conf --config-file /etc/neutron/neutron.conf --config-file /etc/neutron/conf.d/cisco_cfg_agent.ini --config-file /etc/neutron/conf.d/cisco_router_plugin.ini  --config-dir /etc/neutron/conf.d/common --config-dir /etc/neutron/conf.d/neutron-cisco-cfg-agent --log-file /var/log/neutron/cisco-cfg-agent.log
PrivateTmp=false
KillMode=process

[Install]
WantedBy=multi-user.target
